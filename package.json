{
	"name": "omznc/discord-honeypot",
	"module": "index.ts",
	"type": "module",
	"private": true,
	"devDependencies": {
		"@types/bun": "latest"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@biomejs/biome": "^2.3.8",
		"discord.js": "^14.25.1"
	},
	"scripts": {
		"start": "bun run src/bot.ts",
		"check": "bunx --bun @biomejs/biome check --write .",
		"compile:amd64": "bun build src/bot.ts --compile --minify --sourcemap --target=bun-linux-x64 --outfile discord-honeypot-amd64",
		"compile:arm64": "bun build src/bot.ts --compile --minify --sourcemap --target=bun-linux-arm64 --outfile discord-honeypot-arm64",
		"compile:windows": "bun build src/bot.ts --compile --minify --sourcemap --target=bun-windows-x64 --outfile discord-honeypot-windows",
		"compile:macos": "bun build src/bot.ts --compile --minify --sourcemap --target=bun-darwin-x64 --outfile discord-honeypot-macos",
		"compile:macos-arm64": "bun build src/bot.ts --compile --minify --sourcemap --target=bun-darwin-arm64 --outfile discord-honeypot-macos-arm64"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/omznc/discord-honeypot.git"
	},
	"author": "omznc",
	"license": "MIT"
}
